<div id="feeds_wrap">
  <h2>Feeds</h2>
  <button id="add_feed">+</button>
  <form id="new_feed">
    <input placeholder="URL" type="text" name="url" id="new_feed_url"></input>
  </form>
  <ul id="feeds">
    <li id="all">All</li>
  </ul>
</div>

<div id="entries_wrap">
  <h2 id="entries_title">&nbsp;</h2>
  <button id="show_unread">show all</button>
  <ul id="entries">
    <p>
      Add feeds using the <strong>+</strong> button to the left. Click
      a feed to view its contents.
    </p>
  </ul>
</div>
